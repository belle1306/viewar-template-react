{
  "name": "viewar-template-react",
  "version": "0.1.0",
  "description": "A sample application using viewar-api and react",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch --config ./config/webpack.config.js & node dev-scripts/zip-watcher.js --watch",
    "bundle": "npm run build && node dev-scripts/zip-watcher.js",
    "start": "nodemon --watch ./config/webpack.config.js --exec \"webpack-dev-server --open --hot --history-api-fallback --env development --config ./config/webpack.config.js\"",
    "start:mock": "nodemon --watch ./config/webpack.config.js --exec \"webpack-dev-server --open --hot --history-api-fallback --env development_mock --config ./config/webpack.config.js\"",
    "build": "webpack --env production --config ./config/webpack.config.js",
    "deploy": "npm run bundle && npm run quick-deploy",
    "quick-deploy": "node dev-scripts/deploy.js",
    "lint:js": "eslint src/ config/ --cache",
    "stats": "webpack --config ./config/webpack.config.js --env production --profile --json > stats.json"
  },
  "author": "Matthias Klan <mk@viewar.com>",
  "license": "ISC",
  "dependencies": {
    "async-reactor": "^1.2.1",
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "immutable": "^3.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "recompose": "^0.23.5",
    "redux": "^3.7.1",
    "redux-devtools-extension": "^2.13.2",
    "viewar-api": "^0.10.45",
    "viewar-core": "^11.0.0"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.4.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-bluebird": "^1.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chokidar": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0-beta.3",
    "file-loader": "^0.9.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "glob": "^7.1.2",
    "happypack": "^4.0.0-beta.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "ip": "^1.1.5",
    "node-watch": "^0.5.5",
    "nodemon": "^1.11.0",
    "npm-install-webpack-plugin": "^4.0.4",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-apply": "^0.4.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^2.0.6",
    "postcss-responsive-type": "^0.4.0",
    "postcss-smart-import": "^0.7.4",
    "precss": "^2.0.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "request": "^2.81.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0",
    "webpack-uglify-parallel": "^0.1.3"
  }
}
